---
processes:
  - name: service-discovery-controller
    executable: /var/vcap/packages/service-discovery-controller/bin/service-discovery-controller
    args:
      - -c
      - /var/vcap/jobs/service-discovery-controller/config/config.json

    <% if_p('open_files') do %>
    limits:
      open_files: <%= p('open_files') %>
    <% end %>
